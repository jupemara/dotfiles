syntax on
set autoindent
set ambiwidth=double
set backspace=indent,eol,start
set bg=dark
set encoding=utf-8
set fileencoding=utf-8
set hlsearch
set ignorecase
set number
set expandtab
set tabstop=2
set cursorline
highlight CursorLine term=None cterm=None ctermbg=black ctermfg=None
inoremap <silent> jj <ESC> 
